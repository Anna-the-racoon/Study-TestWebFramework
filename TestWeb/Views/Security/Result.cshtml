@model TestWeb.Models.Security

@{
    ViewBag.Title = "Result";

}
@using (Html.BeginForm("Result", "Security", FormMethod.Post))
{
    @Html.ValidationSummary(true)
    //, new { @class = "text-danger" }
    <h3>You input: </h3>
    <div class="mb-3">
        @Html.LabelFor(p => p.Login)
        <br />
        @Html.TextBoxFor(p => p.Login, new { @readonly = "readonly" })
    </div>
    <div>
        @Html.LabelFor(p => p.Password)
        <br />
        @Html.TextBoxFor(p => p.Password, new { @readonly = "readonly" })
    </div>
}